{
  "name": "api-service",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^3.9.0",
    "amqplib": "^0.8.0",
    "axios": "^0.25.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv-safe": "^8.2.0",
    "envalid": "^7.2.2",
    "express": "^4.17.2",
    "helmet": "^5.0.2",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "prisma": "^3.9.0",
    "typescript": "^4.5.5",
    "ubialimv-common": "1.0.0"
  },
  "scripts": {
    "prettier": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint . --ext .ts",
    "prisma:db": "npx prisma db push",
    "prisma:studio": "npx prisma studio",
    "dev": "ts-node-dev --respawn --transpile-only --exit-child src/server.ts",
    "clean:build": "rm -rf dist",
    "copy-contracts": "mkdir -p dist/src/contracts && cp src/contracts/*.yaml dist/src/contracts",
    "build": "yarn run clean:build; tsc -p .;yarn run copy-contracts",
    "start": "ts-node dist/src/server.js"
  },
  "devDependencies": {
    "@types/amqplib": "^0.8.2",
    "@types/axios": "^0.14.0",
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.12",
    "@types/dotenv-safe": "^8.1.2",
    "@types/express": "^4.17.13",
    "@types/helmet": "^4.0.0",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/morgan": "^1.9.3",
    "@types/node": "^17.0.13",
    "@types/swagger-ui-express": "^4.1.3",
    "ts-node": "^10.4.0"
  }
}
